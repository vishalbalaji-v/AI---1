    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 555
        },
        "id": "S6pln62NYXMw",
        "outputId": "c8f38fe4-e947-4801-e90c-3e859e232397"
      },
      "source": [
        "x_train, x_test, y_train, y_test = train_test_split(cars[['Depreciated_Price']],cars['Current_Selling_Price'],train_size = 0.75, random_state = 109)\n",
        "\n",
        "lreg = LinearRegression()\n",
        "\n",
        "lreg.fit(x_train,y_train)\n",
        "y_pred = lreg.predict(x_test)\n",
        "\n",
        "MSE = mean_squared_error(y_test,y_pred)\n",
        "r2 = r2_score(y_test,y_pred)\n",
        "\n",
        "print(f'The Mean Squared Error is {MSE:.4f}')\n",
        "print(f'The r2 score is {r2:.2f}')\n",
        "\n",
        "x_dummy = x_test['Depreciated_Price'].to_numpy()\n",
        "idx = np.argsort(x_dummy)\n",
        "x_dummy = x_dummy[idx]\n",
        "y_pred_1= y_pred[idx]\n",
        "\n",
        "plt.figure(figsize = (12,8))\n",
        "sns.scatterplot(data = cars, x = 'Depreciated_Price', y = 'Current_Selling_Price')\n",
        "plt.plot(x_dummy, y_pred_1, label=\"Polynomial fit\", color='red', alpha=0.6)\n",
        "plt.title('Depreciated Price vs Current Selling Price')\n",
        "plt.show()"
      ],
      "execution_count": 151,
